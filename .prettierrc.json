{
  "printWidth": 200,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "semi": true,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "trailingComma": "none",
  "endOfLine": "lf",
  "embeddedLanguageFormatting": "auto",
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "htmlWhitespaceSensitivity": "strict"
}
